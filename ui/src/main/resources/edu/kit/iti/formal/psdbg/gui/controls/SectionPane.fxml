<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.text.Font?>

<fx:root prefHeight="100" prefWidth="250" styleClass="section-pane" type="javafx.scene.layout.BorderPane" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1">
    <top>
        <HBox fx:id="northBox" alignment="CENTER" styleClass="header-box">
            <Label fx:id="titleLabel" styleClass="title" text="Title"/>
            <Region HBox.hgrow="ALWAYS" />
            <HBox fx:id="buttons" alignment="CENTER" styleClass="header-buttons">
            </HBox>
        </HBox>
    </top>
</fx:root>
